    "@types/node": "^18.0.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vueuse/core": "^8.7.5",
    "c8": "^7.11.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "happy-dom": "^2.55.0",
    "husky": "^8.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unocss": "^0.43.1",
    "unplugin-auto-import": "^0.9.2",
    "vite": "^2.9.13",
